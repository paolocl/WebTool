<?php

namespace ORMBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * AppsOs
 *
 * @ORM\Table(name="apps_os")
 * @ORM\Entity
 */
class AppsOs
{
    /**
     * @var string
     *
     * @ORM\Column(name="name", type="string", length=10)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $name;



    /**
     * Get name
     *
     * @return string
     */
    public function getName()
    {
        return $this->name;
    }
}
